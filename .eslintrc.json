{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "no-await-in-loop": "off",
        "import/extensions": "off",
        "import/order": "off",
        "consistent-return": "off",
        "no-plusplus": "off",
        "react/no-array-index-key": "off",
        "react/prop-types": "off",
        "jsx-a11y/alt-text": "off"
    }
}
